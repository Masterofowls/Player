{
    "version": 2,
    "builds": [
      {
        "src": "main.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/main.html"
      },
      {
        "source": "/artists/(.*)",
        "destination": "/artists/$1"
      }
    ]
  }
  